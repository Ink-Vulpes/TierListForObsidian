{
	"name": "obsidian-sample-plugin",
	"version": "1.0.0",
	"description": "This is a sample plugin for Obsidian (https://obsidian.md)",
	"main": "main.js",
	"scripts": {
		"test": "jest --coverage",
		"sass": "sass --watch --no-source-map --style compressed ./src/styles.scss ./styles.css",
		"dev": "node esbuild.config.mjs",
		"build": "sass --no-source-map --style compressed ./src/styles.scss ./styles.css && tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [],
	"author": "",
	"license": "MIT",
	"devDependencies": {
		"@testing-library/dom": "^8.19.0",
		"@testing-library/react": "^13.4.0",
		"@types/node": "^16.11.6",
		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"jest": "^29.1.2",
		"obsidian": "latest",
		"sass": "^1.55.0",
		"ts-jest": "^29.0.3",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@types/jest": "^29.1.2",
		"jest-environment-jsdom": "^29.1.2",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
